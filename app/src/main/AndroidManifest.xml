<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gaara.bookfinderandroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserManager.LoginActivity" />
        <activity android:name=".UserManager.RegisterActivity" />
        <activity android:name=".BookInfo" />
        <activity android:name=".AdvanceSearchResult" />
        <activity android:name=".BookStoreList" />
        <activity android:name=".UserManager.EmailLogin" />
        <activity android:name=".Search"></activity>
    </application>

</manifest>